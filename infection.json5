{
	"$schema": ".Build/vendor/infection/infection/resources/schema.json",
	"timeout": 10,
	"source": {
		"directories": [
			"Classes"
		],
	},
	"phpUnit": {
		"configDir": "Tests"
	},
	"logs": {
		"stryker": {
			"report": "main"
		},
		"html": ".Build/logs/infection.html",
		"text": ".Build/logs/infection.log"
	},
	"mutators": {
		"@default": true,
        "UnwrapArrayValues": {
            "ignore": [
                // Although technically array_values() might be omitted, it makes clear,
                // which part of the array is passed.
                "Brotkrueml\\MatomoIntegration\\Code\\ScriptTagBuilder::build"
            ]
        }
	}
}
